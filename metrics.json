{
  "revenue": {
    "expression": "COALESCE([REVAmount],0) + COALESCE([WIPAmount],0)",
    "aggregation": "SUM",
    "synonyms": ["revenue", "sales", "turnover", "income"]
  },

  "cost": {
    "expression": "COALESCE([CSTAmount],0) + COALESCE([ACRAmount],0)",
    "aggregation": "SUM",
    "synonyms": ["cost", "expenses", "total cost", "service cost"]
  },

  "profit": {
    "expression": "COALESCE([JobProfit],0)",
    "aggregation": "SUM",
    "synonyms": ["profit", "margin", "jobprofit", "earning"]
  },

  "profit_percentage": {
    "expression": "CASE WHEN SUM(COALESCE([CSTAmount],0) + COALESCE([ACRAmount],0)) = 0 THEN NULL ELSE (SUM(COALESCE([JobProfit],0)) / SUM(COALESCE([CSTAmount],0) + COALESCE([ACRAmount],0))) * 100 END",
    "aggregation": null,
    "synonyms": [
        "profit percent",
        "profit %",
        "margin %",
        "profit ratio",
        "profit percentage",
        "profit pct",
        "profit pct.",
        "profitpercent",
        "profitpercentage",
        "profit_percent",
        "profit%"
    ]
}
,

  "total_job_count": {
    "expression": "COUNT(DISTINCT [JobNumber])",
    "aggregation": null,
    "synonyms": ["job count", "number of jobs", "total jobs", "count of jobs"]
  },

  "total_volume": {
    "expression": "SUM(COALESCE([ActualChargeable],0))",
    "aggregation": null,
    "synonyms": ["volume", "total volume", "sum volume"]
  },

  "usd_revenue": {
    "expression": "COALESCE([USDREVAmount],0)",
    "aggregation": "SUM",
    "synonyms": ["usd revenue", "revenue usd", "converted revenue", "rev usd"]
  },

  "trade_lane_continent": {
    "expression": "COALESCE([OriginContinent],'') + '-' + COALESCE([DestinationContinent],'')",
    "aggregation": null,
    "synonyms": ["continent trade lane", "trade lane continent", "origin-destination continent"]
  },

  "trade_lane_country": {
    "expression": "COALESCE([OriginCountry],'') + '-' + COALESCE([DestinationCountry],'')",
    "aggregation": null,
    "synonyms": ["country trade lane", "trade lane country", "origin-destination country"]
  },

  "arrival_delay_days": {
    "expression": "DATEDIFF(DAY, [DestinationETA], [ConsolATA])",
    "aggregation": "AVG",
    "synonyms": ["arrival delay", "arrival delay days", "delay arrival"]
  },

  "departure_delay_days": {
    "expression": "DATEDIFF(DAY, [OriginETD], [ConsolATD])",
    "aggregation": "AVG",
    "synonyms": ["departure delay", "departure delay days", "delay departure"]
  }
}
